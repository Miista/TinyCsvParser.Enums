    <copyright>Copyright 2023</copyright>
    <tags>TinyCsvParser Parse Generic Enum Converter Mapping</tags>
    <readme>docs\README.md</readme>
    <releaseNotes>Make sure extension methods on ITypeConverterProvider can be chained</releaseNotes>

    <dependencies>
      <group targetFramework="netstandard2.0" />
    </dependencies>
  </metadata>

  <files>
    <file src="netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <file src="netstandard2.0\*.xml" target="lib\netstandard2.0" />
    <file src="..\..\..\..\README.md" target="docs\" />
  </files>
</package>
