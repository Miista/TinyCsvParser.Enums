<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TinyCsvParser.Enums</id>
    <version>1.0.1-alpha</version>
    <title>TinyCsvParser: Parsing enums</title>
    <authors>Søren Guldmund</authors>
    <owners>Søren Guldmund</owners>
    <projectUrl>https://github.com/Miista/TinyCsvParser.Enums</projectUrl>
    <repository type="git" url="https://github.com/Miista/TinyCsvParser.Enums.git" />
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
